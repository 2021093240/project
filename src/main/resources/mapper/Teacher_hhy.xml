<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.TeacherDao_hhy">
    <resultMap id="teacherResultMapper" type="com.example.entity.Teacher_hhy">
        <id column="teacher_id" property="teacherId"/>
    </resultMap>
    <select id="findTeacherByTeacherId" resultMap="teacherResultMapper">
        select * from teacher where teacher_id=#{teacherId}
    </select>
</mapper>