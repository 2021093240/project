<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.CompetentTeacherDao_hhy">
    <resultMap id="competentTeacherResultMapper" type="com.example.entity.Competent_teacher_hhy">
        <id column="id" property="id"/>
        <collection property="teachers" column="teacher_id"  select="com.example.mapper.TeacherDao_hhy.findTeacherByTeacherId" ofType="com.example.entity.Teacher_hhy"/>
    </resultMap>
        <select id="findAllCompetentTeacher" resultMap="competentTeacherResultMapper">
            select * from competent_teacher
        </select>
</mapper>